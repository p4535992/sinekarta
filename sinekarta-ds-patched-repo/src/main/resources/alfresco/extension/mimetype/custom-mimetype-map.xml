<alfresco-config area="mimetype-map">
  <config evaluator="string-compare" condition="Mimetype Map">
    <mimetypes>
      <mimetype mimetype="application/pkcs7-mime" display="PKCS7-MIME">
        <extension>p7m</extension>
      </mimetype>
      <mimetype mimetype="application/pkcs7-signature" display="PKCS7-SIGNATURE">
        <extension>p7s</extension>
      </mimetype>
      <!-- https://github.com/keensoft/alfresco-firma-pdf/blob/master/src/alfresco-components/sign-document/src/main/amp/config/alfresco/extension/mimetype/mimetype-map-xsig.xml -->
      <mimetype mimetype="application/cms" display="CSIG - ASN.1 Signature">
            <extension>csig</extension>
	  </mimetype>
	  <mimetype mimetype="text/xml" display="XSIG - XML Signature">
	        <extension>xsig</extension>
	  </mimetype>
    </mimetypes>
  </config>
</alfresco-config>